h: 750px) {\r\n    * {\r\n        margin: 0;\r\n    }\r\n    .html {\r\n        height: 100%;\r\n    }\r\n\r\n    .content-wrapper {\r\n        height: 100vh;\r\n        overflow-y: hidden;\r\n        flex-direction: row;\r\n    }\r\n\r\n    .card-width {\r\n        min-width: 240px;\r\n        padding-left: 20px;\r\n    }\r\n\r\n    .sedan {\r\n        border-top-right-radius: 0;\r\n        border-bottom-left-radius: 10px;\r\n    }\r\n\r\n    .luxury {\r\n        border-top-right-radius: 10px;\r\n        border-bottom-right-radius: 10px;\r\n        border-bottom-left-radius: 0;\r\n    }\r\n\r\n    .img-logo {\r\n        margin-bottom: 10px;\r\n    }\r\n\r\n    .heading {\r\n        margin-bottom: 30px;\r\n    }\r\n\r\n    .learnMore-sedan:hover {\r\n        background: $background-sedan;\r\n        color: white;\r\n    }\r\n\r\n    .learnMore-suv:hover {\r\n        background: $background-suv;\r\n        color: white;\r\n    }\r\n\r\n    .learnMore-luxury:hover {\r\n        background: $background-luxury;\r\n        color: white;\r\n    }\r\n\r\n    .description {\r\n        max-width: 212px;\r\n        margin-bottom: 30x;\r\n    }\r\n}\r\n\r\n@media all and (min-width: 950px) {\r\n    * {\r\n        margin: 0;\r\n    }\r\n    .html {\r\n        height: 100%;\r\n    }\r\n\r\n    .content-wrapper {\r\n        height: 100vh;\r\n        overflow-y: hidden;\r\n        flex-direction: row;\r\n    }\r\n\r\n    .description {\r\n        max-width: 237px;\r\n        margin-bottom: 50px;\r\n    }\r\n\r\n    .img-logo {\r\n        margin-bottom: 30px;\r\n    }\r\n\r\n    .card-width {\r\n        min-width: 260px;\r\n        padding-left: 40px;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9275c1bb71a7003fdf7243c871611a55","cacheData":{"env":{}}}